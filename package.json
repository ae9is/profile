{
  "name": "profile",
  "version": "0.0.1",
  "private": true,
  "description": "A custom GitHub profile page",
  "license": "MIT",
  "files": [
    "docs"
  ],
  "sideEffects": [
    "*.css",
    "*.png"
  ],
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rm -rf .tsbuildinfo docs",
    "build:vite": "vite build",
    "dev": "vite --port 3000",
    "test": "npm run vitest",
    "vitest": "vitest run",
    "format": "prettier --write src",
    "lint": "eslint src",
    "preview": "vite preview --host 0.0.0.0 --outDir docs",
    "update": "npm run update-packages && npm install && npm run build && npm run test",
    "update-packages": "ncu -u"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.7.0",
    "react": "^18.3.1",
    "react-dice-roll": "^1.2.2",
    "react-dom": "^18.3.1",
    "theme-change": "^2.5.0",
    "vite": "^5.4.9"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^24.1.0",
    "npm-check-updates": "^16.14.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "react-dice-roll": {
      "react": "^18",
      "react-dom": "^18"
    }
  }
}
